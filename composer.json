{
    "name": "df/php-coverfish",
    "description": "lightweight target class coverage check for PHPUnit's CodeCoverage",
    "keywords": ["symfony", "silex", "phpunit", "phpcc", "php", "coverage"],
    "time": "2015-08-08",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/dunkelfrosch/phpcoverfish/issues",
        "email": "coverfish.support@gmail.com"
    },
    "authors": [
        {
            "name": "Patrick Paechnatz",
            "email": "patrick.paechnatz@gmail.com",
            "role": "lead"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "symfony/console": "~2.7",
        "phpunit/php-token-stream": "~1.2",
        "sebastian/finder-facade": "~1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.7"
    },    
    "autoload": {
        "psr-4": {
            "DF\\PHPCoverFish\\": "src/PHPCoverFish/",
            "DF\\PHPCoverFish\\Tests\\": "tests/",
            "DF\\PHPCoverFish\\Tests\\Data\\Src\\": "tests/data/src/",
            "DF\\PHPCoverFish\\Tests\\Data\\Tests\\": "tests/data/tests/"
        }
    },
    "bin": [
       "bin/coverfish"
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "0.9.x-dev"
        }
    }
}
