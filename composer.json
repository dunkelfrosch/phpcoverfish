{
    "name": "df/php-coverfish",
    "description": "lightweight target class coverage check for PHPUnit CodeCoverage analyzer",
    "keywords": ["silex","phpunit","phpcc","php"],
    "license": "MIT",
    "support": {
      "issues": "https://github.com/dunkelfrosch/dfphpcoverfish/issues"
    },
    "authors": [
        {
            "name": "Patrick Paechnatz",
            "email": "patrick.paechnatz@gmail.com",
            "role": "lead"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "symfony/console": "2.7.1",
        "phpunit/php-token-stream": "~1.2",
        "sebastian/finder-facade": "~1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.7"
    },    
    "autoload": {
       "psr-4": {
            "DF\\PHPCoverFish\\": "src/PHPCoverFish/",
            "DF\\PHPCoverFish\\Tests\\Data\\Src\\": "tests/data/src/",
            "DF\\PHPCoverFish\\Tests\\Data\\Tests\\": "tests/data/tests/"
        } 
    },
    "bin": [
      "coverfish"
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.0"
        }
    }
}
